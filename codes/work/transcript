vsim -gui work.regFile
# vsim -gui work.regFile 
# Start time: 20:49:17 on Nov 27,2022
# Loading work.regFile
# Loading work.Reg
add wave -position insertpoint  \
sim:/regFile/reset \
sim:/regFile/register_out \
sim:/regFile/reg_write_low \
sim:/regFile/reg_write_high \
sim:/regFile/reg_readnum_src \
sim:/regFile/reg_readnum_dst \
sim:/regFile/reg_dst_low \
sim:/regFile/reg_dst_high \
sim:/regFile/reg2_read_dst \
sim:/regFile/reg1_read_src \
sim:/regFile/data_to_be_written_low \
sim:/regFile/data_to_be_written_high \
sim:/regFile/data_clk_select \
sim:/regFile/data_actual \
sim:/regFile/clk_Rdst2_input \
sim:/regFile/clk_Rdst2 \
sim:/regFile/clk_Rdst1_input \
sim:/regFile/clk_Rdst1 \
sim:/regFile/clk_actual \
sim:/regFile/clk
force reg_readnum_dst 3'd0
force reg_readnum_src 3'd1
force reset 0
force clk 0
force reg_dst_low 3'd0
force reg_dst_high 3'd1
force data_to_be_written_low 15'd25
force data_to_be_written_high 15'd15
force reg_write_high 0
force reg_write_low 0
run
force reg_write_high 1
force reg_write_low 1
force clk = 1
# Invalid binary digit: =.
# ** UI-Msg: (vsim-4011) Invalid force value: = 1.
# 
force clk  1
run
force reg_write_high 0
force reg_write_low 1
force data_to_be_written_low 15'd100
force -freeze sim:/regFile/clk 1 0, 0 {50 ps} -r 100
run
run
force reg_write_low 0
run
